import java.util.Random;
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num1;
		int num2;
	    int counter;
		int sum =0;
   Random rands = new Random();
   num1= rands.nextInt(6) +1;
   num2= rands.nextInt(6)+1;
   
   sum =num1+num2;
  
  while(num1==num2){
	  
	
	  System.out.printf("Move %d spaces and Continue", sum); 
	  sum += sum;
	 
	   if(num1!=num2){
			
			  
			  System.out.printf("Move %d spaces and stop", sum); 
		
		   }
	  

   }

  


}}