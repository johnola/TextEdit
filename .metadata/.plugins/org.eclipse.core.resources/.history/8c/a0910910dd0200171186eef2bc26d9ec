import java.util.Random;
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num1=1;
		int num2=0;
	    int counter=0;
		int sum =0;
   Random rands = new Random();
   
   
  
  
  while(num1!=num2){
	  num1= rands.nextInt(6) +1;
	   num2= rands.nextInt(6)+1;
	   sum =num1+num2;
	   counter=sum;
	System.out.printf("n1:%d n2:%d\n", num1, num2);
	 
	  System.out.printf("Move %d spaces and Continue\n", sum); 
	  
	 
	   if(num1==num2){
			
			  
			  System.out.printf("Move %d spaces and stop", sum); 
		
		   }
	  
	   sum += counter;
   }

  


}}